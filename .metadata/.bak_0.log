				direction = "up";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldY -= speed/(1.4); 
				}
				break;						
			case "downleft": 
				facingDirectoin = "left";
				direction = "left";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldX -= speed/(1.4); 
				}
				direction = "down";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldY += speed/(1.4); 
				}
				break;
			}
		}		
