		if(currentLife <= 0) {
			if(playerLives > 1) {
				currentLife = maxLife;
				worldX = gp.tileSize * 17;
				worldY = gp.tileSize * 2;
			}
			playerLives--;
		}
		if(playerLives <= 0) {
			gp.gameState = gp.gameOverState;
		}						
	}
	private void movePlayer() {
		// If collision is false, player can move
		if (collisionOn == false && keyH.enterPressed == false ) {
			switch (direction) {
			case "up": worldY -= speed; screenY -= playerScreenYMovement;break;
			case "down": worldY += speed; screenY += playerScreenYMovement;break;
			case "left": worldX -= speed; screenX -= playerScreenXMovement; break;
			case "right": worldX += speed;  screenX += playerScreenXMovement; break;
			
			
			case "upright":
				facingDirectoin = "right";
				direction = "right";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldX += speed/(1.4); 
				}
				direction = "up";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldY -= speed/(1.4); 
				}
				break;
			case "downright":
				facingDirectoin = "right";
				direction = "right";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldX += speed/(1.4); 
				}
				direction = "down";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldY += speed/(1.4); 
				}
				break;
			case "upleft": 
				facingDirectoin = "left";
				direction = "left";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldX -= speed/(1.4); 
				}
				direction = "up";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldY -= speed/(1.4); 
				}
				break;						
			case "downleft": 
				facingDirectoin = "left";
				direction = "left";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldX -= speed/(1.4); 
				}
				direction = "down";
				checkForCollisions();
				if (collisionOn == false && keyH.enterPressed == false ) {
					worldY += speed/(1.4); 
				}
				break;
			}
		}		
	}

