	public void contactMonster(int i) {
		if(i != 999) {
			if (invincible == false && gp.monster[i].dying == false) {
				gp.playSE(3);

//				int damage = gp.monster[i].attack - defense;
				int damage = gp.monster[i].attack;

				if(damage < 0) {
					damage = 0;
				}
				currentLife -= damage;
		 		invincible = true;
			}
		}
	}
	public void damageMonster(int i, int attack) {
		if (i !=999) {
			if(gp.monster[i].invincible == false) {
				gp.playSE(3);
				
				int damage = attack - gp.monster[i].defense;
				if(damage < 0) {
					damage = 0;
				}
				
				gp.monster[i].currentLife -= damage;
				gp.ui.addMessage(damage + " damage!");
				gp.monster[i].invincible = true;
				gp.monster[i].damageReaction();
				if(gp.monster[i].currentLife <= 0) {
					// MONSTER DIES
					gp.monster[i].dying = true;
					gp.ui.addMessage("Killed the " + gp.monster[i].name + "");
					gp.ui.addMessage("+" + gp.monster[i].exp + " exp");
//					NO EXP ADDED VVVV
//					exp += gp.monster[i].exp;
					checkLevelUp();
				}
			}
		}
		else {
//			System.out.aprintln("Miss!");
		}
	}
	private void checkLevelUp() {
		if( exp >= nextLevelExp) {
			level++;
			nextLevelExp = nextLevelExp*2;
			maxLife += 2;
			biteStrength++;
			attack = getAttack();
			defense = getDefense();
			gp.playSE(2);
			gp.gameState = gp.dialogueState;
			gp.ui.currentDialogue = "You are level " + level + " now!\n"
					+ "You feel stronger!";
		}
	}
	public void selectItem() {
		int itemIndex = gp.ui.getItemIndexOnSlot();
		
		if(itemIndex < inventory.size()) {
			Entity selectedItem = inventory.get(itemIndex);
	
			if(selectedItem.type == type_laser || selectedItem.type == type_bubble_shoot) {
				currentWeapon = selectedItem;
				attack = getAttack();
				getPlayerAttackImage();
			}
			if(selectedItem.type == type_shield) {
				currentShield = selectedItem;
				defense = getDefense();
			}
			if(selectedItem.type == type_consumable) {
				selectedItem.use(this);
				inventory.remove(itemIndex);
			}
		}
	}
	public boolean inventoryContains(String objectName) {
		for(int i = 0; i < gp.player.inventory.size(); i++) {
			if(gp.player.inventory.get(i).name == objectName) {
				return true;
			}
		}
		return false;
	}
